#!/usr/bin/env bash
! iwgetid &>> /dev/null && cat /tmp/backup-cal && exit
out=$(gcalcli --conky calw 1 |
sed -e 's/(0\x71(B/‚îÄ/g' \
  -e 's/(0\x78(B/‚îÇ/g' \
  -e 's/(0\x6A(B/‚îò/g' \
  -e 's/(0\x6B(B/‚îê/g' \
  -e 's/(0\x6C(B/‚îå/g' \
  -e 's/(0\x6D(B/‚îî/g' \
  -e 's/(0\x6E(B/‚îº/g' \
  -e 's/(0\x74(B/‚îú/g' \
  -e 's/(0\x75(B/‚î§/g' \
  -e 's/(0\x76(B/‚î¥/g' \
  -e 's/(0\x77(B/‚î¨/g' \
  -e 's/color black/color0/g' \
  -e 's/color red/color1/g' \
  -e 's/color green/color2/g' \
  -e 's/color yellow/color3/g' \
  -e 's/color blue/color4/g' \
  -e 's/color magenta/color5/g' \
  -e 's/color cyan/color6/g' \
	-e 's/color white/color7/g')
[[ "$out" != "" ]] && echo "$out" | tee /tmp/backup-cal
